<VirtualHost *:80>

    ServerName wehaveyourkidneys.com
    ServerAlias www.wehaveyourkidneys.com, pixel.wehaveyourkidneys.com

    DocumentRoot /var/www/we-have-your-kidneys/www

    <Directory "/var/www/we-have-your-kidneys/www">
        Options FollowSymLinks
        AllowOverride All
        Order Allow,Deny
        Allow from all
    </Directory>

</VirtualHost>
